 [emailState, dispatchEmail] = useReducer(emailReducer,{\n    value:'',\n    isValid: null\n  })\n\n  const [passwdState, dispatchPasswd] = useReducer(passwordReducer,{\n    passwd:'',\n    isValid: null\n  })\n  // useEffect(()=>{\n  //   setFormIsValid(\n  //     enteredEmail.includes('@') && enteredPassword.trim().length > 6\n  //   );\n  // },[enteredEmail,enteredPassword])\n\n  const emailChangeHandler = (event) => {\n    // setEnteredEmail(event.target.value);\n    dispatchEmail({type: 'EMAIL_VALUE', val: event.target.value})\n\n    setFormIsValid(\n      emailState.isValid && passwdState.isValid\n    );\n  };\n\n  const passwordChangeHandler = (event) => {\n    // setEnteredPassword(event.target.value);\n    dispatchPasswd({type: 'pass_value', val: event.target.value})\n    setFormIsValid(\n      emailState.isValid && passwdState.isValid\n    );\n  };\n\n  const validateEmailHandler = () => {\n    // setEmailIsValid(emailState.isValid);\n    dispatchEmail({type: 'BLUR_SPACE'})\n  };\n\n  const validatePasswordHandler = () => {\n    // setPasswordIsValid(enteredPassword.trim().length > 6);\n    dispatchPasswd({type: 'no_passwd'})\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    props.onLogin(emailState.value, passwdState.passwd);\n  };\n\n  return (\n    <Card className={classes.login}>\n      <form onSubmit={submitHandler}>\n        <div\n          className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            passwdState.isValid === false ? classes.invalid : ''\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={passwdState.passwd}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\components\\Home\\Home.js",[],"F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\components\\MainHeader\\MainHeader.js",[],"F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\components\\MainHeader\\Navigation.js",[],"F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\components\\UI\\Card\\Card.js",[],"F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\components\\UI\\Button\\Button.js",[],"F:\\React JS\\EffectAndReducer\\EffectAndReducer\\src\\context\\auth-context.js",["43"],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":1,"column":27,"nodeType":"50","messageId":"51","endLine":1,"endColumn":36},{"ruleId":"48","severity":1,"message":"52","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":26},"no-native-reassign",["53"],"no-negated-in-lhs",["54"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useContext' is defined but never used.","no-global-assign","no-unsafe-negation"]